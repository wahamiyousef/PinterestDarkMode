{
    "name": "Pinterest DarkMode",
    "description": "Dark Mode for Pinterest (web)",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://www.pinterest.ca/",
        "https://www.pinterest.com/"
    ]
}